{
  "name": "crowdfunding",
  "version": "0.0.2",
  "main": "server/server.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/dovps/dovps.git"
  },
  "scripts": {
    "watch": "gulp watch",
    "clean": "gulp clean",
    "clean:dep": "gulp clean:dep",
    "deploy:dev": "gulp deploy:dev",
    "deploy:dev:watch": "npm run deploy:dev && npm run watch",
    "deploy:dist": "gulp deploy:dist && npm run db-migrate-up",
    "deploy:dist:watch": "npm run deploy:dist && npm run watch",
    "predeploy:dev": "npm install && npm run update-webdriver && wget -N http://geolite.maxmind.com/download/geoip/database/GeoLiteCity.dat.gz && zcat GeoLiteCity.dat.gz > GeoLiteCity.dat && wget -N http://geolite.maxmind.com/download/geoip/database/GeoLiteCityv6-beta/GeoLiteCityv6.dat.gz && zcat GeoLiteCityv6.dat.gz > GeoLiteCityv6.dat",
    "predeploy:dist": "npm install && npm run update-webdriver && wget -N http://geolite.maxmind.com/download/geoip/database/GeoLiteCity.dat.gz && zcat GeoLiteCity.dat.gz > GeoLiteCity.dat && wget -N http://geolite.maxmind.com/download/geoip/database/GeoLiteCityv6-beta/GeoLiteCityv6.dat.gz && zcat GeoLiteCityv6.dat.gz > GeoLiteCityv6.dat",
    "postinstall": "npm prune && npm update && ./node_modules/bower/bin/bower prune && ./node_modules/bower/bin/bower install && ./node_modules/bower/bin/bower update",
    "test": "gulp test",
    "update-webdriver": "node_modules/protractor/bin/webdriver-manager update",
    "lbng": "node_modules/loopback-sdk-angular-cli/bin/lb-ng server/server.js client/src/scripts/lib/lb-services.js",
    "lbng2": "node_modules/loopback-sdk-angular-cli/bin/lb-ng server/server.js client/src/scripts/lib/lb-services.ts -l angular2 && tsc client/src/scripts/lib/lb-services.ts",
    "db-migrate-create": "node_modules/db-migrate/bin/db-migrate --config migrations/database.json --migrations-dir migrations/migrations --verbose create --sql-file",
    "db-migrate-up": "node_modules/db-migrate/bin/db-migrate --config migrations/database.json --migrations-dir migrations/migrations --verbose up -e `cat server/datasources.json | grep -A 2 postgres | grep database | cut -d'\"' -f 4`",
    "db-migrate-down": "node_modules/db-migrate/bin/db-migrate --config migrations/database.json --migrations-dir migrations/migrations --verbose down -e `cat server/datasources.json | grep -A 2 postgres | grep database | cut -d'\"' -f 4`"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "babel-register": "^6.5.2",
    "body-parser": "~1.15.0",
    "bower": "~1.7.7",
    "db-migrate": "^0.9.23",
    "db-migrate-pg": "^0.1.6",
    "errorhandler": "~1.4.2",
    "express-recaptcha": "^2.0.0",
    "geolib": "^2.0.18",
    "hiredis": "~0.4.1",
    "html-pdf": "^2.0.1",
    "loopback": "~2.27.0",
    "loopback-boot": "~2.17.0",
    "loopback-component-passport": "~2.0.0",
    "loopback-component-storage": "^1.5.1",
    "loopback-connector-postgresql": "~2.4.0",
    "loopback-connector-redis": "~0.0.3",
    "loopback-datasource-juggler": "~2.45.0",
    "maxmind": "^0.6.0",
    "moment": "^2.10.6",
    "moment-timezone": "^0.5.0",
    "morgan": "~1.7.0",
    "mustache": "^2.2.0",
    "passport-facebook": "~2.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "~1.0.0",
    "redis": "~2.4.2",
    "redis-pubsub": "~0.3.1",
    "socket.io": "~1.4.5",
    "socket.io-emitter": "~1.0.0",
    "socket.io-redis": "~1.0.0"
  },
  "optionalDependencies": {
    "loopback-explorer": "~2.0.1"
  },
  "devDependencies": {
    "browser-sync": "^2.9.3",
    "cssnext": "^1.8.4",
    "del": "^2.0.2",
    "glob": "^7.0.0",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.5.2",
    "gulp-connect": "^3.0.0",
    "gulp-csso": "^1.0.0",
    "gulp-debug": "^2.1.0",
    "gulp-develop-server": "^0.5.0",
    "gulp-filenames": "^2.0.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.2.1",
    "gulp-jasmine": "^2.0.1",
    "gulp-jscs": "^3.0.0",
    "gulp-jshint": "^2.0.0",
    "gulp-karma": "^0.0.5",
    "gulp-minify-css": "^1.2.1",
    "gulp-minify-html": "^1.0.4",
    "gulp-postcss": "^6.0.0",
    "gulp-protractor": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.5.1",
    "gulp-size": "^2.0.0",
    "gulp-stylus": "^2.0.6",
    "gulp-subtask": "^0.3.4",
    "gulp-uglify": "^1.4.1",
    "gulp-useref": "^3.0.3",
    "gulp-util": "^3.0.5",
    "jasmine": "^2.3.2",
    "jasmine-core": "^2.3.3",
    "jasmine-reporters": "^2.0.6",
    "jeet": "^6.1.2",
    "jscs": "^2.1.1",
    "jshint": "~2.9.1",
    "jshint-stylish": "^2.0.0",
    "karma": "^0.13.9",
    "karma-jasmine": "^0.3.5",
    "karma-junit-reporter": "^0.3.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "kouto-swiss": "^0.11.12",
    "loopback-component-explorer": "^2.3.0",
    "loopback-sdk-angular-cli": "git://github.com/phra/loopback-sdk-angular-cli.git#37-angular2-support",
    "minijasminenode2": "^1.0.0",
    "phantomjs": "^2.1.3",
    "postcss": "^5.0.6",
    "protractor": "^3.0.0",
    "run-sequence": "^1.1.2",
    "rupture": "^0.6.1",
    "sprity": "^1.0.7",
    "typescript": "^1.8.2"
  }
}
