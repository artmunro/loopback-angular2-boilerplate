module.exports = function(app, cb) {
    app.dataSources.postgres.automigrate(function(err) {
        if (err)
            throw err;
        console.log('POSTGRES AUTOUPDATE DONE');
        app.dataSources.redisDS.automigrate(function(err) {
            if (err)
                throw err;
            console.log('REDIS AUTOUPDATE DONE');
            cb();
        });
    });
};
